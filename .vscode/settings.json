{
  "eslint.validate": ["javascript", "typescript"],
  "cSpell.words": [
    "allowlist",
    "rerender",
    "sprintf",
    "targetings",
    "unauthenticate"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "[javascript]": {
    "editor.defaultFormatter": null
  },
  "[typescript]": {
    "editor.defaultFormatter": null
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": null
  },
  "liveServer.settings.port": 5501
}
